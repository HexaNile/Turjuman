{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "status": 307,
      "headers": {
        "Location": "https://turjuman.netlify.app/login"
      }
    },
    {
      "src": "/(.*)",
      "dest": "index.js",
      "headers": {
        "Cache-Control": "s-maxage=86400"
      }
    }
  ]
}
